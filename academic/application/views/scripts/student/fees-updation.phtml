    if(cols.length > 0){
        table.columns(cols).visible(bool);
     for(i in cols){
         if(!bool)
        $('[data-id="'+cols[i]+'"]').parents("div.input-group").hide();
        else
         $('[data-id="'+cols[i]+'"]').parents("div.input-group").show();
     }
    }
}


