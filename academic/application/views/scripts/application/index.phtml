<script>

    $('button[data-toggle="collapse"]').click(function () {
        $('button[data-toggle="collapse"]').each(function () {
            var id = $(this).data('target');
            $(id).attr({"class": 'collapse', "aria-expanded": 'false'});
            $(this).parent('td').removeAttr("style");
        });

        $(this).parent('td').attr("style", 'background:#2acaca;');


    });


    $('.custom_radio').click(function (e) {
        var enable_panel = $(this).next('input').data('switch');

        $('.custom_radio').each(function () {
            var id = $(this).next('input').data('switch');
            $(id).removeAttr('style');
            $(id).attr('style', 'display:none;');
        });


        $('#term_b_id').removeAttr('required');
        $('#term_id').removeAttr('required');
        $('#course_b').removeAttr('required');
        $('#course').removeAttr('required');


        if (enable_panel == '#both')
        {
            $('#curr').attr('style', 'display:block;');
            $('#back').attr('style', 'display:block;');
            $('#term_b_id').attr('required', 'required');
            $('#term_id').attr('required', 'required');
            $('#course_b').attr('required', 'required');
            $('#course').attr('required', 'required');
        } else {
            $(enable_panel).attr('style', 'display:block;');
            if (enable_panel == '#curr') {
                $('#term_id').attr('required', 'required');
                $('#course').attr('required', 'required');
                $('#term_b_id').removeAttr('required');
                $('#course_b').removeAttr('required');
            } else
            {
                $('#term_b_id').attr('required', 'required');
                $('#course_b').attr('required', 'required');
                $('#term_id').removeAttr('required');
                $('#course').removeAttr('required');
            }
        }
    })


    $('#course_b,#course').keyup(function () {
        var course = $('#course').val();
        var course_b = $('#course_b').val();
        var total = +course;
        total += +course_b;
        $('#total_fee_l').empty();
        $('#total_fee').empty();
        $('#total_fee').val(total);
        $('#total_fee_l').text(total);

    });

    $('#due_date').attr('autocomplete', 'off');
    $(function () {
        $('#due_date').datetimepicker();
    });



    function ConfirmDelete()
    {
        var x = confirm("Are you sure you want to delete?");
        if (x)
            return true;
        else
            return false;
    }


