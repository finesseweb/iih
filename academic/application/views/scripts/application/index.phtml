                    var course = <?php echo (int) $_SESSION['application']['course']; ?>;
                    var course_b = <?php echo (int) $_SESSION['application']['course_b']; ?>;

                    $('#total_fee_l').text(course + course_b);
                    $('#total_fee').val(course + course_b);


