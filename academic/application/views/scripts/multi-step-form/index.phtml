    function readImgURL(input) {
        var fileid = $(input).attr("id");
        if (input.files && input.files[0]) {
        var reader = new FileReader();
       var upload = true;
            reader.onload = function(e) {
                 var type =  e.target.result.split("/")[1];
                 var image = new Image();
                image.src = e.target.result; 
                const regex = /[A-Z]/g;
                
   
   
   
   
                type = type.split(";")[0]
                var size = formatBytes(input.files[0].size);
                const found = size.match(regex)
                var sizetype = found.join("");
                if(type=="pdf" && (parseInt(size)> 100 || sizetype != 'KB' )){
                     alert("File size Cannot exceeds 100 kb");
                     $(input).val("");
                     upload = false;
                }
               
                if(type!="pdf"){
                    if( $(input).parent().next().find('img').length !=0){
                                          image.onload = function() {
                       if(fileid == "photo" && (this.height < 50 || this.width < 50)){
                           alert("Photo should be in correct dimension (50 X 50)px");
                           upload = false;
                          $(input).val("");
                       }
                       else
                       {
                           if(fileid == "photo" && (parseInt(size)> 200 || sizetype != 'KB' )){
                               alert("File size Cannot exceeds 200 kb");
                               $(input).val("");
                           }
                           else
                           {
                           $(input).parent().next().find('img').attr('src', e.target.result);
                           }
                       }
                    }; 
                                
            }
            else{
                $(input).parent().next().append('<img id="eduFile" src="" style="width:5em;" >');
                if(!upload){
                            $(input).val("");
                            return false;
                        }
              $(input).parent().next().find('img').attr('src', e.target.result);  
            }
            $(input).parent().next().find('a').remove();
            }
              else{
                //$(input).parent().next().html('<a href="javascript:0;">pdf file</a>');
                $(input).parent().next().find('img').remove();
            }
            }
            
            
            reader.readAsDataURL(input.files[0]);
            
        }
    }
    $('input[type="file"]').change(function() {
        readImgURL(this);
    });
    
