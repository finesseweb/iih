<script>
    // Added by Kedar 29 Nov 2019 Duck Punching
    $(".chosen-select").chosen();
    
   (function() {
    var _oldAjax = $.ajax;
        $.ajax = function(options) {
            $.extend(options, {
                success: function() {
                    //alert('hello');
                   $('.chosen-select').trigger('chosen:updated');
                   
                },
            statusCode: {
                500: function() {
                    //alert('500 Internal Server Error');
                    $('.load-box').css('display','none');
                }
            }
            });
            return _oldAjax(options);
        };
        
    })();
    //End
    $( document ).ready(function() {

        $(function (){
           $('input[type="text"]').attr('autocomplete','off');
           $('input[type="password"]').attr('autocomplete','off');
        });
    });
    //For Number Only
    $('input[data-toggle="number"]').bind('keyup blur',function(){
 
    var node = $(this);
        if(node.val() == ''){
                node.val('');
        }else{
            node.val(node.val().replace(/[^0-9]/g,'') );
        }
    }); 
    //For string Only
    $('input[data-toggle="albphabets"]').bind('keyup blur',function(){
 
    var node = $(this);
        if(node.val() == ''){
                node.val('');
        }else{
            node.val(node.val().replace(/[^a-z A-Z|\\//,_+.: -]/g,'') );
        }
    }); 
    //For Address string Only
    $('input[data-toggle="address_albphabets"]').bind('keyup blur',function(){
 
    var node = $(this);
        if(node.val() == ''){
                node.val('');
        }else{
            node.val(node.val().replace(/[^a-z A-Z 0-9|\\//,_+.: -]/g,'') );
        }
    }); 
</script>

