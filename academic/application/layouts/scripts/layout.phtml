    <script>




        var interval = '';
        $('.downloadAll').click(function (e)
        {
            $(this).attr('disabled', 'disabled');
            $('#tot_action').remove();
            $('#getstudent').append('<div class = "row" id ="tot_action"><div class="col-md-offset-3 col-md-6"><div class="progress"><div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:0%"> 0% Complete</div></div></div></div>');
            //  $('.progress-bar').css('color','#000');
            var urls = [];
            $('.pdf').each(function (e) {
                if (e < $('.pdf').length - 1) {
                    urls.push($(this).attr('href'));
                } else
                {
                    if (e == $('.pdf').length - 1) {
                        urls.push($(this).attr('href'));
                        interval = setInterval(download, 100, urls, urls.length);
                    }

                }
            });

        });

        function download(urls, toturl) {
            //alert(urls.length);
            var url = urls.pop();
            var a = document.createElement("a");
            a.setAttribute('href', url);
            a.setAttribute('download', '');
            a.setAttribute('target', '_blank');
            a.click();

            if (urls.length == 0) {
                clearInterval(interval);
                $('.downloadAll').removeAttr('disabled');
            }
            {
                var percentage = ((toturl - urls.length) / toturl) * 100;
                $('.progress-bar').attr("aria-valuenow", percentage);
                $('.progress-bar').css("width", percentage + '%');
                $('.progress-bar').empty();
                $('.progress-bar').text((toturl - urls.length) + " of " + toturl + " download initiated " + parseInt(percentage) + "%");

            }

        }

        $('.cancel').click(function () {
            clearInterval(interval);
            $('.downloadAll').removeAttr('disabled');
            // $('#tot_action').remove();
        });



