").trigger("chosen:updated");            });        }})    function ConfirmDelete()    {        var x = confirm("Are you sure you want to delete?");        if (x)            return true;        else            return false;    }    var count = $('#count_val').val(); //Addmore script code    $("body").on("click", "#AddButton", function (event)    {        var count_value = count++;        //alert(count_value);        append_html = '<div class="row">';        append_html += '<div class="col-sm-2"><div class="form-group"><label class="control-label">Fee Heads Name</label><input type="text" name="heads[]" id="heads" class="form-control" /></div></div>';        append_html += '<a href="#" class="removeclass btn btn-primary remove_class" style="padding:7px 15px;margin:24px 15px;">-</a>';        append_html += '</div>';        $('#fields').append(append_html);    });    $("body").on("click", ".removeclass", function (e) {        //alert('sdsd');        $(this).parent('div').remove();        return false;    });    $('#dept_id').change(function () {        var deg = $("#degree_id").val();        var session = $('#session').val();        var dept_id = $('#dept_id').val();        if (deg && session) {            $.ajax({                type: "POST",                url: '<?php echo $this->mainconfig['host'] . 'fee-heads/ajax-get-cat'; ?>',                data: {degree_id: deg, session: session, dept_id: dept_id}            }).done(function (data) {                //alert(data);                $('#feecategory_id').empty();                $('#feecategory_id').html(data);                $(".chosen-select").trigger("chosen:updated");            });        }    });        //To filter Session    $('body').on('change', '#academic_year', function () {        var year_id = $('#academic_year').val();        if (year_id) {            $.ajax({                type: "POST",                url: '<?php echo $this->mainconfig['host'] . 'master/ajax-Get-Session'; ?>',                data: {year_id: year_id}            }).done(function (data) {                $('#session').empty('');                $('#session').html(data);                $('#session').trigger("chosen:updated");                                            });        }    });    $('body').on('change', '#mig_academic_year', function () {        var year_id = $('#mig_academic_year').val();        if (year_id) {            $.ajax({                type: "POST",                url: '<?php echo $this->mainconfig['host'] . 'master/ajax-Get-Session'; ?>',                data: {year_id: year_id}            }).done(function (data) {                $('#mig_session').empty('');                $('#mig_session').html(data);                $('#mig_session').trigger("chosen:updated");                                            });        }    });    $('body').on('change', '#degree_id', function () {       $('.load-box').css('display','block');        var degree = $(this).val();        if (degree) {            $.ajax({                type: "POST",                url: '<?php echo $this->mainconfig['host'] . 'plan/ajax-get-stream-by-degree'; ?>',                data: {degree: degree}            }).done(function (data) {                $('#dept_id').empty();                $('#dept_id').html(data).trigger("chosen:updated");                $('.load-box').css('display','none');            });        }    });    //TO GET FEE HEADS NAME    $('body').on('change', '#feecategory_id', function () {        $('.load-box').css('display','block');        var cat_id= $(this).val();        var degree  =   $('#degree_id').val();        var dept_id =   $('#dept_id').val();        var session =   $('#session').val();        if (degree) {            $.ajax({                type: "POST",                url: '<?php echo $this->mainconfig['host'] . 'fee-heads/ajax-get-fee-head-name'; ?>',          